- content_for :page_title do
  = t('errors.410.title')

- content_for :content do
  = t('errors.410.content')

- content_for :error_code do
  = 410

- content_for :static_image do
  = error_asset_url(t('errors.410.static_image')) unless t('errors.410.static_image').nil?
  = error_asset_url('images/errors/410-static.png') if t('errors.410.static_image').nil?

- content_for :dynamic_image do
  = error_asset_url(t('errors.410.dynamic_image')) unless t('errors.410.dynamic_image').nil?
  = error_asset_url('images/errors/410-dynamic.png') if t('errors.410.dynamic_image').nil?